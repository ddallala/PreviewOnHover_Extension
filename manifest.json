{
    "manifest_version": 2,
    "name": "Dynamics 365 Preview On Hover",
    "version": "0.1",
    "content_scripts": [
        {
            "all_frames": true,
            "matches": [
                "https://*.dynamics.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        "preview-on-hover/*"
    ]
}